{
  "name": "workouterr-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "g:api": "./bin/generate-api.sh",
    "g:icons": "svg-to-ts-object --config ./.svg-to-tsrc-lib.json",
    "u:tailwind": "npm install tailwindcss@latest postcss@latest autoprefixer@latest @tailwindcss/typography@latest @tailwindcss/forms@latest @tailwindcss/aspect-ratio@latest @tailwindcss/line-clamp@latest @tailwindcss/container-queries --save-exact",
    "prettier:all": "prettier --write ./src/**/*.{html,ts,scss}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/cdk": "^16.2.11",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.0.7",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.0.7",
    "@angular/router": "^17.0.7",
    "@ngneat/transloco": "^6.0.0",
    "@tailwindcss/container-queries": "0.1.1",
    "inquirer-directory": "^2.2.0",
    "inquirer-file-path": "^1.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "~17.0.7",
    "@angular/compiler-cli": "^17.0.7",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.10",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "10.4.16",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "plop": "4.0.1",
    "postcss": "8.4.33",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "svg-to-ts": "10.1.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.2.2"
